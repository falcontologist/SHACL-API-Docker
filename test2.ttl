@prefix : <http://example.org/ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Test instances for opaque property inference
:testAcquisition1 a :Acquisition ;
    :lemma "acquire" ;
    :synset "develop or internalize a new ability, skill, or preference" ;
    :acquirer :alice ;
    :acquisition :skill1 .

:testAcquisition2 a :Acquisition ;
    :lemma "acquire" ;
    :synset "take on, display, or exhibit a characteristic" ;
    :acquirer :bob ;
    :acquisition :quality1 .

:testAcquisition3 a :Acquisition ;
    :lemma "acquire" ;
    :synset "become the owner or possessor of something" ;
    :acquirer :charlie ;
    :acquisition :property1 .

:testAcquisition4 a :Acquisition ;
    :lemma "learn" ;
    :synset "gain knowledge or understanding of something through study or experience" ;
    :acquirer :diana ;
    :acquisition :knowledge1 .

:testAcquisition5 a :Acquisition ;
    :lemma "purchase" ;
    :synset "obtain something by paying for it" ;
    :acquirer :eve ;
    :acquisition :product1 .

# Test instances with same lemma+synset combination (should produce same opaque property)
:testAcquisition6 a :Acquisition ;
    :lemma "acquire" ;
    :synset "develop or internalize a new ability, skill, or preference" ;
    :acquirer :frank ;
    :acquisition :skill2 .

:testAcquisition7 a :Acquisition ;
    :lemma "acquire" ;
    :synset "develop or internalize a new ability, skill, or preference" ;
    :acquirer :grace ;
    :acquisition :skill3 .

# Entities
:alice a :Entity ;
    rdfs:label "Alice"@en .

:bob a :Entity ;
    rdfs:label "Bob"@en .

:charlie a :Entity ;
    rdfs:label "Charlie"@en .

:diana a :Entity ;
    rdfs:label "Diana"@en .

:eve a :Entity ;
    rdfs:label "Eve"@en .

:frank a :Entity ;
    rdfs:label "Frank"@en .

:grace a :Entity ;
    rdfs:label "Grace"@en .

:skill1 a :Entity ;
    rdfs:label "Programming Skill"@en .

:skill2 a :Entity ;
    rdfs:label "Writing Skill"@en .

:skill3 a :Entity ;
    rdfs:label "Public Speaking Skill"@en .

:quality1 a :Entity ;
    rdfs:label "Patience"@en .

:property1 a :Entity ;
    rdfs:label "House"@en .

:knowledge1 a :Entity ;
    rdfs:label "Ancient History"@en .

:product1 a :Entity ;
    rdfs:label "Laptop"@en .

# Test instance that should NOT generate opaque property (missing lemma/synset)
:testAcquisitionIncomplete a :Acquisition ;
    :acquirer :hank ;
    :acquisition :item1 .

:hank a :Entity ;
    rdfs:label "Hank"@en .

:item1 a :Entity ;
    rdfs:label "Mystery Item"@en .

# Test instance that should generate simple inference but NOT opaque property
:testAcquisitionNoSynset a :Acquisition ;
    :lemma "obtain" ;
    :acquirer :ian ;
    :acquisition :item2 .

:ian a :Entity ;
    rdfs:label "Ian"@en .

:item2 a :Entity ;
    rdfs:label "Item Two"@en .

# Alphabet acquires Wiz test case (for determinism proof)
:acquisitionAlphabetWiz a :Acquisition ;
    :lemma "acquire" ;
    :synset "become the owner or possessor of something" ;
    :acquirer :alphabet ;
    :acquisition :wiz .

# Google acquires YouTube test case (same lemma+synset, should produce same opaque IRI)
:acquisitionGoogleYouTube a :Acquisition ;
    :lemma "acquire" ;
    :synset "become the owner or possessor of something" ;
    :acquirer :google ;
    :acquisition :youtube .

# New entities for these tests
:alphabet a :Entity ;
    rdfs:label "Alphabet"@en .

:wiz a :Entity ;
    rdfs:label "Wiz"@en .

:google a :Entity ;
    rdfs:label "Google"@en .

:youtube a :Entity ;
    rdfs:label "YouTube"@en .
